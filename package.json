{
  "name": "@neilbmclaughlin/rules-engine-test",
  "version": "5.0.0",
  "description": "Test of json-rules-engine for demo ELM caculation service",
  "main": "ffc-rules-engine.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "pretest": "npm run lint",
    "lint": "standard",
    "test": "jest",
    "test:watch": "nodemon --inspect=0.0.0.0 --legacy-watch --exec npm test",
    "examples": "node examples/examples.js"
  },
  "author": "Neil McLaughlin",
  "license": "MIT",
  "devDependencies": {
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "standard": "^14.3.1"
  },
  "standard": {
    "globals": [
      "describe",
      "beforeEach",
      "expect",
      "fail",
      "test",
      "afterEach",
      "jest",
      "beforeAll",
      "afterAll"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      "**/test-output/**"
    ],
    "ext": "js,json"
  },
  "dependencies": {
    "json-rules-engine": "^5.0.2",
    "jsonschema": "^1.2.5",
    "moment": "^2.24.0",
    "verror": "^1.10.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neilbmclaughlin/rules-engine-test.git"
  },
  "bugs": {
    "url": "https://github.com/neilbmclaughlin/rules-engine-test/issues"
  },
  "homepage": "https://github.com/neilbmclaughlin/rules-engine-test#readme"
}
